<form id="profileEdit">
  <div class="form-group">
    <label for="firstName">Имя</label>
    <input type="text" class="form-control" id="firstName" value="{{user.firstName}}" maxlength="20">
  </div>
  <div class="form-group">
    <label for="lastName">Фамилия</label>
    <input type="text" class="form-control" id="lastName" value="{{user.lastName}}" maxlength="20">
  </div>
  <div class="form-group">
    <label for="birthDate">Дата рождения</label>
    <input type="date" class="form-control" id="birthDate" value="{{user.birthDate}}">
  </div>
  <div class="form-group">
    <label for="phone">Телефон</label>
    <input type="text" class="form-control" id="phone" value="{{user.phone}}" maxlength="20">
  </div>
  <div class="form-group">
    <label for="email">Email</label>
    <input type="email" class="form-control" id="email" value="{{user.email}}" maxlength="20">
  </div>
  <button type="submit" class="btn btn-primary">Сохранить</button>
</form>

<script defer src="/js/profileEdit.js"></script>
